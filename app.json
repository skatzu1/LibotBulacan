{
  "expo": {
    "name": "Libot",
    "slug": "libot",
    "version": "1.0.0",
    "scheme": "libot",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "newArchEnabled": true,
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "bundler": {
      "metro": {
        "assetExts": [
          "glb",
          "gltf",
          "bin"
        ]
      }
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-asset",
      [
        "@reactvision/react-viro",
        {
          "android": {
            "xRMode": [
              "AR"
            ]
          },
          "ios": {
            "cameraUsagePermission": "Libot uses the camera for AR experiences.",
            "microphoneUsagePermission": "Libot uses your microphone for AR experiences.",
            "photosPermission": "Libot would like to read photos for AR.",
            "savephotosPermission": "Libot would like to save photos during AR."
          }
        }
      ],
      "expo-secure-store",
      "expo-web-browser"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Libot uses the camera to show augmented reality tourism content.",
        "NSLocationWhenInUseUsageDescription": "Find AR objects nearby at tourist locations"
      }
    },
    "android": {
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.skatzu15.libot"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8f2cc8dd-553e-49c8-a0c4-cfe299f6489f"
      }
    }
  }
}
